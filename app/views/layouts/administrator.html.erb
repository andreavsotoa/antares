<!DOCTYPE html>
<html>
<head>
  <title>CONDOMINET</title>
  <%= javascript_include_tag "administrator" %>
  
  <%= stylesheet_link_tag 'administrator', media: 'all', 'data-turbolinks-track' => true %>
  
  <%= csrf_meta_tags %>
  
  <%= favicon_link_tag 'favicon.ico', :rel => 'favicon.ico', :type => 'image/png', :sizes => '144x144' %>

</head>
<body class="padTop53 ">
    <!-- MAIN WRAPPER -->
    <div id="wrap" >
        

        <!-- HEADER SECTION -->
        <div id="top">

            <nav class="navbar navbar-inverse navbar-fixed-top " style="padding-top: 10px;">
                <a data-original-title="Show/Hide Menu" data-placement="bottom" data-tooltip="tooltip" class="accordion-toggle btn btn-primary btn-sm visible-xs" data-toggle="collapse" id="menu-toggle">
                    <i class="icon-align-justify"></i>
                </a>
                <!-- LOGO SECTION -->
                <header class="navbar-header">
                    <a href="index.html" class="navbar-brand">
                    <%=image_tag "logo.png" %>
                    </a>
                </header>
                <!-- END LOGO SECTION -->
                <ul class="nav navbar-top-links navbar-right">

                    <!-- MESSAGES SECTION -->
                        <%= render 'shared/messages_section' %>
                    <!--END MESSAGES SECTION -->

                    <!--task section -->
                        <%= render 'shared/task_section' %>
                    <!--end task section -->

                    <!--ALERTS SECTION -->
                         <%= render 'shared/alert_section' %>
                    <!-- END ALERTS SECTION -->


                    <!--ADMIN SETTINGS SECTIONS -->
                         <%= render 'shared/admin_setting' %>
                     <!--END ADMIN SETTINGS -->
                </ul>

            </nav>

        </div>
        <!-- END HEADER SECTION -->

        <!-- MENU SECTION -->
            <%= render 'shared/menu' %>
        <!--END MENU SECTION -->

        <!--PAGE CONTENT -->
        <div id="content">
             
            <div class="inner" style="min-height: 700px;">
                <div class="row">
                    <div class="col-lg-12">
                        <h1> Adminitraci√≥n </h1>
                    </div>
                </div>
                <hr />
                <%= yield %>       
            </div>

        </div>
        <!--END PAGE CONTENT -->

    </div> 
    <!--END MAIN WRAPPER -->

    <!-- RIGHT STRIP  SECTION --> 
      <%= render 'shared/right_strip' %>
    <!-- END RIGHT STRIP  SECTION -->


    <!-- FOOTER -->
      <%= render 'shared/footer' %>
    <!--END FOOTER -->


</body>
</html>
